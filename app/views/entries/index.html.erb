<h1>All Entries</h1>
<div>
<table>
    <tr>
    <th>Name</th>
    <th>Link</th>
    <th>Notes</th>
    </tr>
    <% @entries.each do |entry| %>
    <tr>
    <td>
   <%= entry.name %>
   </td>
   <td>
   <%= entry.link %>
   </td>
   <td>
   <%= entry.notes %>
   </td>
   <td>
   <%= button_to "Delete", entry, method: :delete, data: { confirm: "Are you sure you want to delete this Project ?"} %>
   </td>
   </tr>
 <% end %>
 </table>
</div>